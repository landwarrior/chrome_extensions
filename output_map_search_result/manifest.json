{
    "manifest_version": 3,
    "name": "Googleマップ検索結果CSV出力",
    "version": "1.0",
    "description": "Googleマップの検索結果をCSVに出力する拡張機能",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": [
        "https://www.google.co.jp/maps/*",
        "https://www.google.com/maps/*"
    ],
    "action": {
        "default_popup": ""
    },
    "background": {
        "service_worker": "background.js"
    }
}
